{
  "name": "vite-vue-starter",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:f": "npm run dev:frontend",
    "dev:b": "npm run dev:backend",
    "dev": "npm run dev:both",
    "dev:frontend": "cd public_facing && npm run dev",
    "dev:backend": "cd private_facing && npm run dev",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
