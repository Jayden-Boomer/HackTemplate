{
  "name": "vite-vue-starter",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "public": "cd public_facing && npm run dev",
    "private": "cd private_facing && npm run dev",
    "both": "concurrently \"npm run dev:private\" \"npm run dev:public\"",
    "copy-envs": "concurrently \"cd public_facing && cp -n .env.example .env\" \"cd private_facing && cp -n .env.example .env\"",
    "npm-install-all": "npm install && concurrently \"cd public_facing && npm install\" \"cd private_facing && npm install\"",
    "setup": "npm run copy-envs && npm run npm-install-all"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "concurrently": "^8.2.2",
    "npm": "^11.6.2"
  }
}
